{
  "description": "Read your favorite news for free.",
  "manifest_version": 2,
  "name": "TheLocal Unlimited",
  "version": "1.0.1",
  "homepage_url": "https://github.com/oussa/thelocal-unlimited",

  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.thelocal.de/*"],
      "css": ["styles.css"]
    }
  ]
}
