{
  "description": "Read your favorite news for free.",
  "manifest_version": 3,
  "name": "TheLocal Unlimited",
  "version": "1.2.0",
  "homepage_url": "https://github.com/oussa/thelocal-unlimited",

  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "content_scripts": [
    {
      "matches": [
        "*://*.thelocal.at/*",
        "*://*.thelocal.ch/*",
        "*://*.thelocal.com/*",
        "*://*.thelocal.de/*",
        "*://*.thelocal.dk/*",
        "*://*.thelocal.es/*",
        "*://*.thelocal.fr/*",
        "*://*.thelocal.it/*",
        "*://*.thelocal.no/*",
        "*://*.thelocal.se/*"
      ],
      "css": ["styles.css"]
    }
  ]
}
